    "expo-analytics-amplitude": "7.0.0",
    "expo-asset": "7.0.0",
    "expo-av": "7.0.0",
    "expo-barcode-scanner": "7.0.0",
    "expo-blur": "7.0.0",
    "expo-brightness": "7.0.0",
    "expo-camera": "7.0.0",
    "expo-constants": "7.0.0",
    "expo-font": "7.0.0",
    "expo-linear-gradient": "7.0.0",
    "expo-local-authentication": "7.0.0",
    "expo-permissions": "7.0.0",
    "expo-sensors": "7.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "1.4.1",
    "react-native-maps": "0.25.0",
    "react-native-reanimated": "1.2.0",
    "react-native-screens": "1.0.0-alpha.23",
    "react-native-svg": "9.9.2",
    "react-navigation": "4.0.6",
    "react-navigation-stack": "1.8.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@types/expo__vector-icons": "9.0.1",
    "@types/react": "16.8.25",
    "@types/react-native": "0.60.15",
    "@types/react-navigation": "3.0.8",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "@unimodules/core": "4.0.0",
    "babel-preset-expo": "7.0.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.14.3",
    "expo-cli": "3.0.10",
    "prettier": "1.18.2",
    "typescript": "3.6.3"
  }
